<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>PassageList</servlet-name>
    <servlet-class>passage.PassageList</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>UrlList</servlet-name>
    <servlet-class>passage.UrlList</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Addpassage</servlet-name>
    <servlet-class>passage.Addpassage</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>PassageList</servlet-name>
    <url-pattern>/servlet/PassageList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UrlList</servlet-name>
    <url-pattern>/servlet/UrlList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Addpassage</servlet-name>
    <url-pattern>/servlet/Addpassage</url-pattern>
  </servlet-mapping>

</web-app>